<?xml version="1.0"?>
<block>
  <name>RS map Generator</name>
  <key>lte_rs_map_generator_m</key>
  <category>lte</category>
  <import>import lte</import>
  <make>lte.rs_map_generator_m("cell_id", "pilots", $N_rb_dl, $ant_port)</make>

  <param>
    <name>resource blocks</name>
    <key>N_rb_dl</key>
    <type>int</type>
  </param>
  
  <param>
    <name>antenna port</name>
    <key>ant_port</key>
    <type>int</type>
  </param>

  <sink>
    <name>cell_id</name>
    <type>message</type>
  </sink>

  <source>
    <name>pilots</name>
    <type>message</type>
  </source>
  
</block>
