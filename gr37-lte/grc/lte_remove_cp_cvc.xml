<?xml version="1.0"?>
<block>
  <name>Remove CP</name>
  <key>lte_remove_cp_cvc</key>
  <category>lte</category>
  <import>import lte</import>
  <make>lte.remove_cp_cvc($fftl, $key)</make>

  <param>
    <name>FFT length</name>
    <key>fftl</key>
    <type>int</type>
  </param>
  
  <param>
    <name>tag key value</name>
    <key>tag_key</key>
    <type>string</type>
  </param>


  <sink>
    <name>in</name>
    <type>complex</type>
  </sink>


  <source>
    <name>out</name>
    <type>complex</type>
    <vlen>$fftl</vlen>
  </source>
  
</block>
